#!groovy
// Run docker build
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    stages {
        stage("create docker images") {
            steps {
                echo " ============== docker build start =================="
                sh "dir"
                sh "kubectl apply -f ."
                echo " ============== docker build stop =================="
                }
            }
        }
}

