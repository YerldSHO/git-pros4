#!groovy
// Run docker build
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    stages {

        stage('k8s build'){
            steps{
                echo " ============== k8s build start ==================="

                sh "kubectl port-forward my-web 8000:8000"
                echo " ============== k8s build stop =================="
            }
        }
   }
}


